{
  "manifest_version": 3,
  "name": "Image Host Replacer (MangaPark Only)",
  "version": "1.1",
  "description": "Replaces img url to load img correctly .",
  "content_scripts": [
    {
      "matches": [
        "https://mangapark.io/*",
        "https://mangapark.com/*",
        "https://mangapark.net/*",
        "https://mangapark.org/*",
        "https://mangapark.me/*",
        "https://mangapark.to/*",
        "https://comicpark.org/*",
        "https://comicpark.to/*",
        "https://readpark.org/*",
        "https://readpark.net/*",
        "https://parkmanga.com/*",
        "https://parkmanga.net/*",
        "https://parkmanga.org/*",
        "https://mpark.to/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
